{
  "name": "culturelens",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup": "node scripts/setup.js",
    "dev": "next dev",
    "dev:backend": "node scripts/start-backend.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:backend\" --names \"frontend,backend\" --prefix-colors \"cyan,magenta\"",
    "build": "next build",
    "start": "next start",
    "lint:frontend": "eslint . --ext .ts,.tsx",
    "lint:backend": "cd backend && source venv/bin/activate && ruff check .",
    "format:frontend": "prettier --write .",
    "format:backend": "cd backend && source venv/bin/activate && ruff format .",
    "format:check": "prettier --check .",
    "test:backend": "cd backend && source venv/bin/activate && pytest -v",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
